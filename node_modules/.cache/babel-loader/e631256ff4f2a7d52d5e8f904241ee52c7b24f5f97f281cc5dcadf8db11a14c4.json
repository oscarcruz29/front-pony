{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport eyeClosedIcon from '@/assets/closed.png';\nimport eyeOpenIcon from '@/assets/open.png';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      showPassword: false,\n      showConfirmPassword: false,\n      email: '',\n      username: '',\n      password: '',\n      confirmPassword: '',\n      backgroundImage: require('@/assets/fondo.png'),\n      // Ruta de la imagen de fondo \n      eyeClosedIcon,\n      eyeOpenIcon\n    };\n  },\n  methods: {\n    togglePasswordVisibility() {\n      this.showPassword = !this.showPassword;\n      const passwordInput = document.getElementById('passwordInput');\n      passwordInput.type = this.showPassword ? 'text' : 'password';\n    },\n    toggleConfirmPassword() {\n      this.showConfirmPassword = !this.showConfirmPassword;\n      const confirmPasswordInput = document.getElementById('confirmPasswordInput');\n      confirmPasswordInput.type = this.showConfirmPassword ? 'text' : 'password';\n    },\n    async register() {\n      try {\n        const response = await axios.post('http://127.0.0.1:8000/api/registrar', {\n          name: this.username,\n          email: this.email,\n          password: this.password\n        });\n        if (response.data.msg === 'success') {\n          // Redirigir al login\n          this.$router.push('/login');\n        } else {\n          alert('Error en el registro');\n        }\n      } catch (error) {\n        alert('Error al registrar: ' + error.response.data.msg);\n      }\n    },\n    handleSubmit() {\n      this.register();\n    }\n  }\n};","map":{"version":3,"names":["eyeClosedIcon","eyeOpenIcon","axios","data","showPassword","showConfirmPassword","email","username","password","confirmPassword","backgroundImage","require","methods","togglePasswordVisibility","passwordInput","document","getElementById","type","toggleConfirmPassword","confirmPasswordInput","register","response","post","name","msg","$router","push","alert","error","handleSubmit"],"sources":["C:\\Users\\corre\\OneDrive\\Documents\\Amaro\\PonyLink\\ponylink-frontend\\src\\components\\RegisterComponent.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Registro</h1>\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div>\r\n        <label for=\"username\">Nombre de usuario:</label>\r\n        <input type=\"text\" v-model=\"username\" id=\"username\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"email\">Correo electrónico:</label>\r\n        <input type=\"email\" v-model=\"email\" id=\"email\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"password\">Contraseña:</label>\r\n        <input :type=\"showPassword ? 'text' : 'password'\" v-model=\"password\" id=\"passwordInput\" required>\r\n        <button type=\"button\" @click=\"togglePasswordVisibility\">\r\n          <img :src=\"showPassword ? eyeOpenIcon : eyeClosedIcon\" alt=\"Toggle Password Visibility\">\r\n        </button>\r\n      </div>\r\n      <div>\r\n        <label for=\"confirmPassword\">Confirmar Contraseña:</label>\r\n        <input :type=\"showConfirmPassword ? 'text' : 'password'\" v-model=\"confirmPassword\" id=\"confirmPasswordInput\" required>\r\n        <button type=\"button\" @click=\"toggleConfirmPassword\">\r\n          <img :src=\"showConfirmPassword ? eyeOpenIcon : eyeClosedIcon\" alt=\"Toggle Confirm Password Visibility\">\r\n        </button>\r\n      </div>\r\n      <button type=\"submit\">Registrar</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport eyeClosedIcon from '@/assets/closed.png';\r\nimport eyeOpenIcon from '@/assets/open.png';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      showPassword: false,\r\n      showConfirmPassword: false,\r\n      email: '',\r\n      username: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      backgroundImage: require('@/assets/fondo.png'), // Ruta de la imagen de fondo \r\n      eyeClosedIcon,\r\n      eyeOpenIcon\r\n    }\r\n  },\r\n  methods: {\r\n    togglePasswordVisibility() {\r\n      this.showPassword = !this.showPassword;\r\n      const passwordInput = document.getElementById('passwordInput');\r\n      passwordInput.type = this.showPassword ? 'text' : 'password';\r\n    },\r\n    toggleConfirmPassword() {\r\n      this.showConfirmPassword = !this.showConfirmPassword;\r\n      const confirmPasswordInput = document.getElementById('confirmPasswordInput');\r\n      confirmPasswordInput.type = this.showConfirmPassword ? 'text' : 'password';\r\n    },\r\n    async register() {\r\n      try {\r\n        const response = await axios.post('http://127.0.0.1:8000/api/registrar', {\r\n          name: this.username,\r\n          email: this.email,\r\n          password: this.password\r\n        });\r\n        \r\n        if (response.data.msg === 'success') {\r\n          // Redirigir al login\r\n          this.$router.push('/login');\r\n        } else {\r\n          alert('Error en el registro');\r\n        }\r\n      } catch (error) {\r\n        alert('Error al registrar: ' + error.response.data.msg);\r\n      }\r\n    },\r\n    handleSubmit() {\r\n      this.register();\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";AAgCA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,eAAe,EAAEC,OAAO,CAAC,oBAAoB,CAAC;MAAE;MAChDX,aAAa;MACbC;IACF;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,wBAAwBA,CAAA,EAAG;MACzB,IAAI,CAACT,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;MACtC,MAAMU,aAAY,GAAIC,QAAQ,CAACC,cAAc,CAAC,eAAe,CAAC;MAC9DF,aAAa,CAACG,IAAG,GAAI,IAAI,CAACb,YAAW,GAAI,MAAK,GAAI,UAAU;IAC9D,CAAC;IACDc,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAACb,mBAAkB,GAAI,CAAC,IAAI,CAACA,mBAAmB;MACpD,MAAMc,oBAAmB,GAAIJ,QAAQ,CAACC,cAAc,CAAC,sBAAsB,CAAC;MAC5EG,oBAAoB,CAACF,