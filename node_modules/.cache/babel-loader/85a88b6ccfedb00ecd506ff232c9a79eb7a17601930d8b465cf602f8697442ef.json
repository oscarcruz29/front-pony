ndleSubmit() {\n      this.register();\n    }\n  }\n};","map":{"version":3,"names":["eyeClosedIcon","eyeOpenIcon","axios","data","showPassword","showConfirmPassword","email","username","password","confirmPassword","backgroundImage","require","methods","togglePasswordVisibility","passwordInput","document","getElementById","type","toggleConfirmPasswordVisibility","confirmPasswordInput","register","alert","response","post","name","msg","$router","push","error","status","handleSubmit"],"sources":["C:\\Users\\corre\\OneDrive\\Documents\\Amaro\\PonyLink\\ponylink-frontend\\src\\components\\RegisterComponent.vue"],"sourcesContent":["<template>\r\n  <!-- Overlay con bg-black y bg-opacity-40 -->\r\n  <div class=\"absolute inset-0 bg-black bg-opacity-40\"></div>\r\n  <div class=\"relative flex flex-col items-center px-10 pt-10 w-full max-md:px-4 max-md:max-w-full\">\r\n    <main class=\"flex z-10 flex-col px-10 pt-6 pb-16 max-w-full bg-white rounded-xl shadow-md w-[460px] max-md:px-6 max-md:mb-4\">\r\n      <!-- Imagen superior más grande pero ajustada -->\r\n      <div class=\"w-[200px] h-[200px] rounded-full overflow-hidden mx-auto mb-6 shadow-[0px_4px_4px_rgba(0,0,0,0.45)]\">\r\n        <img loading=\"lazy\" src=\"https://cdn.builder.io/api/v1/image/assets/TEMP/2fe73ee0b7d23c2c13d1e5eca1344c4389f603bac3093fea44ff18f46ac4ff25?placeholderIfAbsent=true&apiKey=55e491f87d61487b9b7b3861f7502d13\" alt=\"Company logo\" class=\"w-full h-full object-cover\" />\r\n      </div>\r\n      <h1 class=\"self-start text-xl font-semibold text-slate-900 max-md:ml-1.5 mb-5\">\r\n        Crear Cuenta\r\n      </h1>\r\n      <form @submit.prevent=\"handleSubmit\" class=\"w-full\">\r\n        <label for=\"nameInput\" class=\"sr-only\">Nombre</label>\r\n        <input id=\"nameInput\" v-model=\"username\" type=\"text\" placeholder=\"Nombre\" class=\"w-full px-4 py-4 mt-3 text-base rounded-md border-b border-sky-900 bg-neutral-100 focus:outline-none focus:ring-2 focus:ring-sky-900 focus:border-tran